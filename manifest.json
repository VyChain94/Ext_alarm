{ 
    "name": "Awareness tracker",
    "description": "This is alarming!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },

    "permissions": [
        "alarms", 
        "notifications"
    ],
    
    "action": {
      "default_popup": "popup.html"
    }
  }